{
  "name": "twigstarter",
  "description": "Webpack 4 static pages template",
  "version": "0.0.1",
  "keywords": [
    "webpack",
    "twig",
    "sass"
  ],
  "author": {
    "name": "Andrzej Wojtczyk",
    "email": "andrzej@wojtczyk.pl",
    "web": "https://wojtczyk.pl"
  },
  "bugs": "https://github.com/awojtczyk/awostarter/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/awojtczyk/awostarter.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "lint:js": "./node_modules/.bin/eslint src/js/*.js",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:html": "pa11y-ci ./src/**/*.html",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js --watch-poll",
    "start": "webpack --config webpack.config.prod.js && http-server ./dist -o",
    "deploy": "webpack --config webpack.config.prod.js && ./deploy",
    "init": "./init"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "9.2.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.5",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "expose-loader": "^0.7.5",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "jquery": "^3.3.1",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pa11y-ci": "^2.1.1",
    "plop": "^2.1.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "twig-html-loader": "^0.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "foundation-sites": "^6.5.0-rc.4",
    "js-cookie": "^2.2.0"
  }
}
